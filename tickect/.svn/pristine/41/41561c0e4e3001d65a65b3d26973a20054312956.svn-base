package com.xasxt.tickect.vo;
public class AjaxResp {
	private int code;
	private Object result;
	private String message;

	/**
	 * 
	 * @return 0--正常应答;-99--系统异常;-1--无权限;-2--未提供有效参数
	 */
	public int getCode() {
		return code;
	}

	/**
	 * 应答码
	 * 
	 * 0--正常应答;-99--系统异常;-1--无权限;-2--未提供有效参数
	 * @param code
	 */
	public void setCode(int code) {
		this.code = code;
	}

	/**
	 * 应答结果数据
	 * @return
	 */
	public Object getResult() {
		return result;
	}

	/**
	 * 应答结果数据
	 * @param result
	 */
	public void setResult(Object result) {
		this.result = result;
	}

	/**
	 * 异常消息
	 * @return
	 */
	public String getMessage() {
		return message;
	}

	/**
	 * 应答结果数据
	 * @param message
	 */
	public void setMessage(String message) {
		this.message = message;
	}

}

